   "source": [
    "import socket\n",
    "\n",
    "host = 'localhost'\n",
    "port = 4444\n",
    "\n",
    "with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:\n",
    "    s.connect((host, port))\n",
    "    message = b'Hello, server!'\n",
    "    s.sendall(message)\n",
    "    response = s.recv(1024)\n",
    "    print('Response:', response.decode())\n"
   ]
